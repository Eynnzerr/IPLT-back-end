<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.relearnspring.mapper.PositionMapper">
    <select id="testPositionList" resultType="com.example.relearnspring.model.Position">
        SELECT * FROM position limit 10;
    </select>
    
    <select id="getPosByBatch" resultType="com.example.relearnspring.model.Position">
        SELECT * FROM position WHERE sample_batch = #{batch};
    </select>
</mapper>